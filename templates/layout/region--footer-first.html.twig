{#
/**
 * @file
 * Theme override to display a region.
 *
 * Available variables:
 * - content: The content for this region, typically blocks.
 * - attributes: HTML attributes for the region div.
 * - region: The name of the region variable as defined in the theme's
 *   .info.yml file.
 *
 * @see template_preprocess_region()
 */
#}
{%
  set classes = [
    'region',
    'region-' ~ region|clean_class,
  ]
%}
<section id="footer_first" {{ attributes.addClass(classes) }}>
  <a href="/" title="Home" rel="home" class="navbar-brand">
    <img src="/themes/contrib/idc_ui_theme_boots/images/logo_white.svg" alt="Home" class="site-logo">
  </a>
</section>
{# {% if content %}
  <section{{ attributes.addClass(classes)|merge({ '#attributes': { 'class': [ 'hidden' ] } }) }}>
    {{ content }}
  </section>
{% endif %} #}
